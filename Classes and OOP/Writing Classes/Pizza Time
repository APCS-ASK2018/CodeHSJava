public class Pizza
{
    // Write your class here!
    private String type;
    private String toppings;
    private int size;
    
    public Pizza(String theType, String theToppings, int theSize) {
        type = theType;
        toppings = theToppings;
        size = theSize;
    }
    
    public String toString() {
        return "Type: " + type + " Toppings: " + toppings + " Size: " + size;
    }
}
